{
  "table_name": "users",
  "relationships": {
    "one_to_one": {
      "user_profiles": {
        "foreign_key": "user_id",
        "on_delete": "CASCADE",
        "on_update": "CASCADE",
        "description": "Setiap user memiliki satu profil"
      }
    },
    "one_to_many": {
      "user_sessions": {
        "foreign_key": "user_id",
        "on_delete": "CASCADE",
        "on_update": "CASCADE",
        "description": "User dapat memiliki multiple sesi login"
      },
      "user_activities": {
        "foreign_key": "user_id",
        "on_delete": "CASCADE",
        "on_update": "CASCADE",
        "description": "Log aktivitas user"
      },
      "password_resets": {
        "foreign_key": "user_id",
        "on_delete": "CASCADE",
        "on_update": "CASCADE",
        "description": "Request reset password untuk user"
      },
      "user_roles": {
        "foreign_key": "user_id",
        "on_delete": "CASCADE",
        "on_update": "CASCADE",
        "description": "Assign role ke user"
      },
      "audit_logs": {
        "foreign_key": "user_id",
        "on_delete": "SET NULL",
        "on_update": "CASCADE",
        "description": "Log audit yang dibuat oleh user"
      }
    },
    "many_to_many": {
      "roles": {
        "through_table": "user_roles",
        "foreign_key": "user_id",
        "related_foreign_key": "role_id",
        "description": "User dapat memiliki multiple roles"
      },
      "permissions": {
        "through_table": "user_roles -> role_permissions",
        "description": "User dapat memiliki permissions melalui roles"
      }
    }
  },
  "relationship_constraints": {
    "user_profiles": {
      "constraint_name": "fk_user_profiles_user_id",
      "references": "users(id)",
      "on_delete": "CASCADE"
    },
    "user_sessions": {
      "constraint_name": "fk_user_sessions_user_id",
      "references": "users(id)",
      "on_delete": "CASCADE"
    },
    "user_activities": {
      "constraint_name": "fk_user_activities_user_id",
      "references": "users(id)",
      "on_delete": "CASCADE"
    },
    "password_resets": {
      "constraint_name": "fk_password_resets_user_id",
      "references": "users(id)",
      "on_delete": "CASCADE"
    },
    "user_roles": {
      "constraint_name": "fk_user_roles_user_id",
      "references": "users(id)",
      "on_delete": "CASCADE"
    },
    "audit_logs": {
      "constraint_name": "fk_audit_logs_user_id",
      "references": "users(id)",
      "on_delete": "SET NULL"
    }
  }
}